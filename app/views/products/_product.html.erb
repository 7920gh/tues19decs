<div id="<%= dom_id product %>">
  <p>
    <strong>Title:</strong>
    <%= product.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= product.description %>
  </p>

  <p>
    <strong>Image url:</strong>
    <%= product.image_url %>
<img src="<%= product.image_url %>"></img>

<%= image_tag(<%= product.image_url %>) %>


  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

</div>
